{
  "compilerOptions": {
    //"incremental": true,
    "listEmittedFiles": true,
    "outDir": "./tsjs/",
    "strict": true, /* Enable all strict type-checking options. */

    "lib": ["ES2016"], // no DOM
    "target": "ES2016",
    "typeRoots": [
      "node_modules/@types"
    ],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "sourceMap": true
  },
  "exclude": [
      "node_modules"
  ]
}
